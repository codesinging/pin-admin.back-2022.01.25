{
    "name": "codesinging/pin-admin",
    "description": "Rapidly build administrative application based on Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "codesinging",
            "email": "codesinging@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.62"
    },
    "autoload": {
        "psr-4": {
            "CodeSinging\\PinAdmin\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "^6.21"
    },
    "autoload-dev": {
        "psr-4": {
            "CodeSinging\\PinAdmin\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeSinging\\PinAdmin\\Foundation\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "CodeSinging\\PinAdmin\\Facades\\Admin"
            }
        }
    }
}
